<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/design_default_color_on_primary</item>
        <item name="colorPrimaryDark">@color/design_default_color_primary_dark</item>
        <item name="colorAccent">@color/teal_200</item>
        <item name="android:colorAccent">@color/teal_200</item>

    </style>

    <style name="CornerSize">
        <item name="cornerSize">50%</item>
    </style>

    <style name="AlertDialogRTL" parent="ThemeOverlay.AppCompat.Dialog.Alert">
        <item name="android:layoutDirection">rtl</item>
    </style>

</resources>